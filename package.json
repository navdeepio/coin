{
  "name": "@navxio/coin",
  "description": "An experimental command line application to track/manage your cryptocurrency portfolio across different exchanges",
  "version": "0.1.9",
  "author": "Nav Saini @navdeepio",
  "bin": {
    "coin": "./bin/run"
  },
  "bugs": "https://github.com/navdeepio/coin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-update": "^1.3.9",
    "axios": "^0.18.0",
    "ccxt": "^1.17.502",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.0.0",
    "coin-ticker": "^3.2.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.1",
    "listr": "^0.14.3",
    "ora": "^3.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4.2.0",
    "debug": "^4.1.0",
    "eslint": "^5.5",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/navdeepio/coin",
  "keywords": [
    "oclif",
    "cli",
    "cryptocurrency"
  ],
  "license": "GPL-3.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "coin",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update"
    ],
    "hooks": {}
  },
  "repository": "navdeepio/coin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "dev-ticker": "DEBUG=worker:* bin/run ticker unsupported_exchange eth"
  }
}
